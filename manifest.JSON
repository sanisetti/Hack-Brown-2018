{
  "manifest_version" : 2,
  "name": "Pupil",
  "description": "todo",
  "version" : "0.1",
  "browser_action": {
    "default_icon": "img/logo_48.png"
  },
  "icons": {
    "16": "img/logo_16.png",
    "48": "img/logo_48.png",
    "128": "img/logo_128.png" },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery.js", "content.js", "requester.js"],
    "run_at": "document_start"
  }
]
}
